{
  "name": "react-production",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./src/shared/config/webpack/webpack.config.ts",
    "start": "cross-env NODE_ENV=development webpack serve --config ./src/shared/config/webpack/webpack.config.ts",
    "preview": "npm run build && serve build",
    "lint:ts": "eslint ./src/**",
    "fix:ts": "eslint ./src/** --fix",
    "lint:styles": "stylelint \"**/*.scss\"",
    "fix:styles": "stylelint --fix \"**/*.scss\"",
    "stylelint-scss-check": "stylelint-config-prettier-scss-check",
    "format": "prettier . --write && npm run fix:ts && npm run fix:styles",
    "lint:all": "npm run lint:ts && npm run lint:styles",
    "test": "jest --config ./src/shared/config/jest/jest.config.ts"
  },
  "devDependencies": {
    "@eslint/js": "9.2.0",
    "@svgr/webpack": "8.1.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.7",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@types/webpack": "5.28.5",
    "@types/webpack-dev-server": "4.7.2",
    "cross-env": "7.0.3",
    "css-loader": "7.1.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-i18next": "6.0.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "file-loader": "6.2.0",
    "globals": "15.1.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "mini-css-extract-plugin": "2.9.0",
    "prettier": "3.2.5",
    "sass": "1.75.0",
    "sass-loader": "14.2.1",
    "style-loader": "4.0.0",
    "stylelint": "^16.5.0",
    "stylelint-config-clean-order": "5.4.2",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "13.1.0",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "typescript-eslint": "7.8.0",
    "webpack": "5.91.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "dotenv": "16.4.5",
    "i18next": "23.11.3",
    "i18next-browser-languagedetector": "7.2.1",
    "i18next-http-backend": "2.5.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "14.1.1",
    "react-router-dom": "6.23.0"
  }
}
